            </OnPlatform>
            <OnPlatform x:Key="MontserratLight" x:TypeArguments="x:String">
                <On Platform="iOS" Value="Montserrat-Light.otf"/>
                <On Platform="Android" Value="Montserrat-Light.otf#Montserrat-Light."/>
            </OnPlatform>
            <OnPlatform x:Key="MontserratRegular" x:TypeArguments="x:String">
                <On Platform="iOS" Value="Montserrat-Regular.otf"/>
                <On Platform="Android" Value="Montserrat-Regular.otf#Montserrat-Regular."/>
            </OnPlatform>

